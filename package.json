{
  "name": "react-3d-cube",
  "version": "1.0.4",
  "description": "cube app using react & react-motion.",
  "keywords": [
    "react",
    "react-motion",
    "3d",
    "cube"
  ],
  "author": "pjb0811",
  "license": "MIT",
  "repository": "pjb0811/react-3d-cube",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "@types/react-motion": "^0.0.27",
    "react-motion": "^0.5.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "babel-core": "6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "7.0.3",
    "gh-pages": "3.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts-ts": "4.0.3",
    "rollup": "2.60.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss-modules": "2.0.2",
    "rollup-plugin-typescript2": "0.31.1",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "4.5.2"
  },
  "files": [
    "dist"
  ]
}
